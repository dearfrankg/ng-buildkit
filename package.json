{
  "name": "ng-buildkit",
  "version": "0.1.1",
  "description": "Gulp starter with common tasks and scenarios",
  "repository": {
    "type": "git",
    "url": "git://github.com/greypants/gulp-starter.git"
  },
  "browser": {
    "angular": "./app/bower_components/angular/angular.js",
    "angular-ui-router": "./app/bower_components/angular-ui-router/release/angular-ui-router.js",
    "lodash": "./node_modules/lodash/lodash.js",
    "plugin": "./app/modules/vendor/jquery-plugin.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify",
      "hbsfy"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    },
    "angular": {
      "exports": "angular"
    },
    "lodash": {
      "exports": "_"
    }
  },
  "devDependencies": {
    "browser-sync": "~1.3.6",
    "browserify": "~5.10.1",
    "browserify-shim": "~3.7.0",
    "coffee-script": "^1.8.0",
    "coffeeify": "~0.7.0",
    "coffeelint": "^1.6.0",
    "coffeelint-stylish": "^0.1.1",
    "gulp": "^3.8.7",
    "gulp-changed": "^0.4.1",
    "gulp-coffeelint": "^0.4.0",
    "gulp-compile-handlebars": "^0.2.0",
    "gulp-concat": "^2.3.4",
    "gulp-imagemin": "^0.6.2",
    "gulp-jshint": "^1.8.4",
    "gulp-ng-html2js": "^0.1.8",
    "gulp-notify": "^1.4.2",
    "gulp-rename": "^1.2.0",
    "gulp-ruby-sass": "^0.7.1",
    "gulp-util": "^3.0.0",
    "handlebars": "^1.3.0",
    "hbsfy": "~2.0.0",
    "jshint": "^2.5.5",
    "jshint-stylish": "^0.4.0",
    "karma": "^0.12.23",
    "karma-chrome-launcher": "^0.1.4",
    "karma-coffee-preprocessor": "^0.2.1",
    "karma-firefox-launcher": "^0.1.3",
    "karma-jasmine": "^0.2.2",
    "karma-phantomjs-launcher": "^0.1.4",
    "pretty-hrtime": "~0.2.1",
    "require-dir": "^0.1.0",
    "vinyl-source-stream": "~0.1.1",
    "watchify": "~1.0.2"
  },
  "dependencies": {
    "jquery": "~1.x",
    "lodash": "^2.4.1"
  }
}
